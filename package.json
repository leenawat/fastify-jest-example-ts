{
  "name": "fastify-jest-example-ts",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "directories": {
    "test": "jest"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "start": "npm run build:ts && cross-env NODE_ENV=develop fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "dev": "tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc --watch\" \"cross-env NODE_ENV=develop fastify start --ignore-watch=\\\".next logs .ts$\\\" -w -l info -P dist/app.js\"",
    "migration:up": "db-migrate up",
    "migration:down": "db-migrate down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.6",
    "db-migrate": "^0.11.13",
    "db-migrate-mysql": "^2.2.0",
    "fastify": "^3.24.0",
    "fastify-autoload": "^3.3.1",
    "fastify-cli": "^2.13.0",
    "fastify-jwt": "^4.0.0",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.0",
    "knex": "^0.95.14",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "tsc-node": "0.0.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "0.0.40",
    "@types/jest": "^27.0.3",
    "@types/knex": "^0.16.1",
    "@types/node": "^15.0.0",
    "@types/tap": "^15.0.0",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "fastify-tsconfig": "^1.0.1",
    "jest": "^27.3.1",
    "tap": "^15.0.9",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
